<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
<Appenders>
    <Console name="Console" target="SYSTEM_OUT">
        <PatternLayout pattern="===>[LOG_DATE:%d{yyyy-MM-dd HH:mm:ss}{GMT-3}][LEVEL:%-5p][CLASS:%c{1}] - %m%n"/>
    </Console>
</Appenders>
<Loggers>
    <Root level="info">
        <AppenderRef ref="Console"/>
    </Root>
</Loggers>
</Configuration>
